<!--<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"-->
        <!--"http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">-->
<module rename-to="MySampleApplication">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.i18n.I18N" />
    <inherits name="com.google.gwt.xml.XML" />
    <inherits name='com.extjs.gxt.ui.GXT'/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <set-property name="gwt.logging.logLevel" value="ALL"/>             # set the default logLevel
    <set-property name="gwt.logging.enabled" value="TRUE"/>             # enable logging
    <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>   # enable console Handler
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />   # prevents gwt popup to display log messages

    <!--<set-property name="gwt.logging.simpleRemoteHandler" value="ENABLED" />-->

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.mySampleApplication.client.MySampleApplicationEntryPoint'/>


    <!-- Specify the app servlets.                   -->
    <!--<servlet path='/MySampleApplication.jsp' class='org.apache.jasper.servlet.JspServlet'/>-->

</module>